{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": ["prettier", "plugin:prettier/recommended", "eslint:recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false
      }
    ]
  }
}
